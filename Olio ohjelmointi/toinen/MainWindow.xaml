<Window x:Class="toinen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:toinen"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="txt_Ekateksti" HorizontalAlignment="Left" Margin="274,139,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Grid.RowSpan="2"/>
        <TextBlock x:Name="txt_Tokanappi" HorizontalAlignment="Left" Margin="367,139,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" RenderTransformOrigin="0.308,3.199" Grid.RowSpan="2"/>
        <Button x:Name="bdn_Ekanappi" Content="Button" HorizontalAlignment="Left" Margin="274,97,0,0" VerticalAlignment="Top" Click="bdn_Ekanappi_Click"/>
        <Button x:Name="bdn_Tokanappi" Content="Button" HorizontalAlignment="Left" Margin="372,97,0,0" VerticalAlignment="Top" Click="bdn_Tokanappi_Click"/>

    </Grid>
</Window>
